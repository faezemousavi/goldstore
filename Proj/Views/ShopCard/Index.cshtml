
@{
    ViewBag.Title = "فاکتور";
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}

<div id="showOrder">
    @Html.Action("Order")
</div>


<script>
    function commandOrder(id,count)
    {
        $.ajax({
            url: "/ShopCard/CommandOrder/" + id,
            type: "Get",
            data: { count: count }
        }).done(function (res) {
            $("#showOrder").html(res);
        });
    }
</script>